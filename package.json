{
  "name": "express-rest-mongo",
  "version": "0.0.20",
  "url": "https://www.npmjs.com/package/express-rest-mongo",
  "description": "Mongo prebaked with authentication and a Rest API!",
  "main": "lib/express-rest-mongo.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcrowder65/express-rest-mongo"
  },
  "keywords": [
    "express",
    "node",
    "rest",
    "mongo",
    "mongodb",
    "api"
  ],
  "scripts": {
    "pretest": "mongodb-runner start",
    "linter": "eslint src && eslint test",
    "test": "npm run linter && npm run mocha",
    "posttest": "mongodb-runner stop",
    "build": "babel src -d lib",
    "example": "babel-watch example/index.js -w src",
    "prod": "npm run build && node lib/express-rest-mongo.js",
    "mocha": "istanbul cover --include-all-sources --root src --report text --report lcov node_modules/mocha/bin/_mocha -- -c --opts mocha.opts test",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "pre-commit": [
    "build"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "http-status-codes": "^1.2.0",
    "jsonwebtoken": "^7.4.3",
    "mongodb": "^2.2.31"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.0.20",
    "babel-eslint": "^8.0.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babel-watch": "^2.0.7",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.5.0",
    "eslint-config-walmart": "^1.2.4",
    "eslint-plugin-filenames": "^1.2.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "mongodb-runner": "^3.6.1",
    "pre-commit": "^1.2.2",
    "unique-string": "^1.0.0"
  }
}
